        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment>Specifies a brief description of the attack step technique</rdfs:comment>
        <rdfs:label>Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#techniqueId -->

    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#techniqueId">
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#AttackStepTechnique"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Specifies an identifier for the technique</rdfs:comment>
        <rdfs:label>Technique Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#techniqueName -->

    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#techniqueName">
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#ObfuscationTechnique"/>
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#ProbingTechnique"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies a name associated with the technique</rdfs:comment>
        <rdfs:label>Technique Name</rdfs:label>
    </owl:DatatypeProperty>
    


