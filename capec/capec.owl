    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#keywords">
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#AttackPattern"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies one or more keywords.  Keyword correspond to text strings used to tag and search CAPEC catalog data.</rdfs:comment>
        <rdfs:label>keywords</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#localReferenceId -->

    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#localReferenceId">
