    <!-- http://capec.mitre.org/capec#AttackPhase -->

    <owl:Class rdf:about="http://capec.mitre.org/capec#AttackPhase">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://capec.mitre.org/capec#attackSteps"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://capec.mitre.org/capec#phaseId"/>
