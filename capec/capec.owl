                                                                                <rdf:rest>
                                                                                    <rdf:Description>
                                                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RequiredBy</rdf:first>
                                                                                        <rdf:rest>
                                                                                            <rdf:Description>
                                                                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Requires</rdf:first>
                                                                                                <rdf:rest>
                                                                                                    <rdf:Description>
                                                                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">StartsChain</rdf:first>
                                                                                                        <rdf:rest>
                                                                                                            <rdf:Description>
                                                                                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">StartsWith</rdf:first>
                                                                                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                                                                            </rdf:Description>
                                                                                                        </rdf:rest>
                                                                                                    </rdf:Description>
                                                                                                </rdf:rest>
                                                                                            </rdf:Description>
                                                                                        </rdf:rest>
                                                                                    </rdf:Description>
                                                                                </rdf:rest>
                                                                            </rdf:Description>
                                                                        </rdf:rest>
                                                                    </rdf:Description>
                                                                </rdf:rest>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the nature of the relationship between this element and the target element, such as ChildOf, HasMember or Requires to name a few.</rdfs:comment>
        <rdfs:label>relationship nature</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#relationshipNotes -->

    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#relationshipNotes">
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#Category"/>
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#CompoundElement"/>
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#View"/>
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment>Specifies details regarding the relationships between CAPEC entries.</rdfs:comment>
        <rdfs:label>Relationship Notes</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#relationshipTargetForm -->

    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#relationshipTargetForm">
        <rdfs:domain rdf:resource="http://capec.mitre.org/capec#RelationshipStatement"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
