        >1</owl:maxCardinality>
        <owl:onProperty>
          <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#regionalInternetRegistry">
            <rdfs:domain rdf:resource="#AutonomousSystem"/>
