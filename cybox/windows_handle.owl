<?xml version="1.0"?>
<rdf:RDF
    xmlns:winHandleObj="http://cybox.mitre.org/objects/WindowsHandle#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://cybox.mitre.org/objects/WindowsHandle">
  <owl:Ontology rdf:about="http://cybox.mitre.org/objects/WindowsHandle">
    <rdfs:label >Cyber Observable eXpression (CybOX) Windows Handle Ontology</rdfs:label>
    <rdfs:comment >This ontology provides a vocabulary and representation of a Windows Handle for the  Cyber Observable eXpression (CybOX).  &#xD;
&#xD;
CybOX  provides a common mechansim (structure and content) to convey the full range of potential cyber observable information and strives to be fully expressive, flexible, extensible, automatable, and as human-readable as possible.&#xD;
&#xD;
The ontology is based on the Cybox Windows Handle object schema maintained by The MITRE Corporation copyright 2012-2014.  The version number of the ontology is kept in sync with the schema in order to more easily relate between the ontology and the corresponding schema version.&#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2.1.0-000</owl:versionInfo>
    
    <owl:imports rdf:resource="http://cybox.mitre.org/cybox_core"/>
    <owl:imports rdf:resource="http://cybox.mitre.org/cyboxCommon"/>
	
  </owl:Ontology>
  <owl:Class rdf:ID="WindowsHandle">
    <rdfs:subClassOf rdf:resource="http://cybox.mitre.org/cybox_core#Object"/>
 <!--   <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#hasChanged"/>        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
-->	
    <rdfs:label >Windows Handle</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
		<owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="handleID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#description"/>
      </owl:Restriction>
    </rdfs:subClassOf>
-->
    
    
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#physicalLocation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
-->	
    <rdfs:comment >Characterize Windows Handles.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="handleName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="pointerCount"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#discoveryMethod"/>
      </owl:Restriction>
    </rdfs:subClassOf>
-->	
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="accessMask"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#state"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    
-->	
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#definedEffect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
-->	
    <rdfs:subClassOf>	
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="handleType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cyboxCommon#id"/>
      </owl:Restriction>
    </rdfs:subClassOf>  --> 
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="objectAddress"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
	
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="#accessMask">
    
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the access bitmask of the handle.</rdfs:comment>
    <rdfs:label >Access Mask</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#handleID">
    
    <rdfs:label >Handle ID</rdfs:label>
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the unique number used to identify the handle.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#objectAddress">
    
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the address of the Windows object that the handle refers to.</rdfs:comment>
    <rdfs:label >Object Address</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#handleType">
    
    <rdfs:comment >Specifies the handle type, which is equivalent to the type of Windows object that the handle refers to.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Handle Type</rdfs:label>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#pointerCount">
    
    <rdfs:label >Pointer Count</rdfs:label>
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the count of pointer references to the Windows object that the handle refers to.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#handleName">
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WindowsHandle"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the name of the handle.</rdfs:comment>
    <rdfs:label >Handle Name</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

