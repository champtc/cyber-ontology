  <owl:ObjectProperty rdf:about="#handle">
    <rdfs:label >Handle</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsPipe"/>
    <rdfs:comment >Specifies the open Windows handle to the pipe.</rdfs:comment>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#openMode">
    <rdfs:label >Open Mode</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsPipe"/>
    <rdfs:comment >Specifies the open mode used for the pipe.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#named">
    <rdfs:label >Named</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Pipe"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment >Specifies whether the pipe is named.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#pipeName">
    <rdfs:label >Pipe Name</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Pipe"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the name of the pipe, if applicable.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#defaultTimeout">
