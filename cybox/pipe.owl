    <rdfs:comment >Specifies the mode used for the pipe.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#maxInstances">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    <rdfs:comment >Specifies the maximum number of instances that can be created for this pipe.</rdfs:comment>
    <rdfs:domain rdf:resource="#WindowsPipe"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Max Instance</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#inBufferSize">
