  <owl:ObjectProperty rdf:about="#handle">
    <rdfs:label >Handle</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle"/>
    <rdfs:domain rdf:resource="#WindowsThread"/>
    <rdfs:comment >Specifies the handle of a specific thread. See also: http://msdn.microsoft.com/en-us/library/windows/desktop/ms682453(v=vs.85).aspx.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#context">
    <rdfs:label >Context</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WindowsThread"/>
    <rdfs:comment >Specifies the thread context structure, which contains processor-specific register data.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#threadID">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    
    <rdfs:label >Thread ID</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:comment >Specifies the identifier of this thread.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#runningStatus">
    
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:label >Running Status</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Specifies the running state that the thread is in.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#startAddress">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:label >Start Address</rdfs:label>
    <rdfs:comment >Specifies the start address of this thread, representing the memory address where this thread should start.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#parameterAddress">
    <rdfs:label >Parameter Address</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:comment >Specifies the address of the parameters passed to the start address.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#stackSize">
    <rdfs:label >Stack Size</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:comment >Specifies  the stack size of the thread. &#xD;
&#xD;
See also: http://msdn.microsoft.com/en-us/library/windows/desktop/ms686774(v=vs.85).aspx.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#creationDateTime">
    <rdfs:label >Creation Time</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    
    <rdfs:comment >Specifies the creation time of the thread.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#priority">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    
    <rdfs:label >Priority</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#Thread"/>
    <rdfs:comment >Specifies the priority of the thread.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#securityAttributes">
    <rdfs:label >Security Attributes</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsThread"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the security attributes for the thread. See also: http://msdn.microsoft.com/en-us/library/windows/desktop/aa379560(v=vs.85).aspx.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#creationFlags">
    <rdfs:label >Creation Flags</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsThread"/>
    <rdfs:comment >Specifies the creation flags that a thread may be launched with. See also: http://msdn.microsoft.com/en-us/library/windows/desktop/ms684863(v=vs.85).aspx.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
  </owl:DatatypeProperty>
