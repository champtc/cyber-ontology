  <owl:ObjectProperty rdf:about="#vendor.details">
    <rdfs:label >Vendor Details</rdfs:label>
    <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
    <rdfs:comment >Specifies a reference to the vendor that holds the software copyright of the package.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#epoch">
    <rdfs:comment >Specifies the epoch number of the package.</rdfs:comment>
    
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Epoch</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#evr">
    <rdfs:label >EVR</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the epoch, version, and release fields of the package as a single version string.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#release">
    <rdfs:label >Release</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the release number of the package build.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#vendorName">
    <rdfs:label >Vendor Name</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the vendor that holds the software copyright of the package.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#architecture">
    <rdfs:comment >Specifies the architecture for which the package was built.</rdfs:comment>
    
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Architecture</rdfs:label>
    
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#category">
    <rdfs:label >Category</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the categories under which a package may be displayed.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#version">
    <rdfs:label >Version</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the version number of the package build.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#packageName">
    <rdfs:comment >Specifies the name of the package.</rdfs:comment>
    
    <!-- <rdfs:domain rdf:resource="#LinuxPackage"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Name</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

