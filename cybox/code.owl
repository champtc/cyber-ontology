  xml:base="http://cybox.mitre.org/objects/Code">
  <owl:Ontology rdf:about="http://cybox.mitre.org/objects/Code">
    <owl:imports rdf:resource="http://cybox.mitre.org/cybox_core"/>
    
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1.0.0-000</owl:versionInfo>
    <rdfs:comment >This ontology provides a vocabulary and representation of Code for the  Cyber Observable eXpression (CybOX).  &#xD;
&#xD;
CybOX  provides a common mechansim (structure and content) to convey the full range of potential cyber observable information and strives to be fully expressive, flexible, extensible, automatable, and as human-readable as possible.&#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <rdfs:label >Cyber Observable eXpression (CybOX) Code Ontology</rdfs:label>
    <owl:imports rdf:resource="http://cpe.mitre.org/cpe"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Code">
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="codeSegment"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="codeType"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="startAddress"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
