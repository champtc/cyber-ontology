  <owl:ObjectProperty rdf:ID="targetedPlatforms">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:label >targeted platforms</rdfs:label>
    <rdfs:comment >Specifies a list platforms that this code is targeted for.</rdfs:comment>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#PlatformSpecification"/>
          <rdf:Description rdf:about="http://cpe.mitre.org/cpe#Platform"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#codePurpose">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Code Purpose</rdfs:label>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    
    <rdfs:comment >Specifies the purpose or flavor of code being characterized.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeLanguage">
    <rdfs:label >Code Language</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifes the code language used in the code.</rdfs:comment>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeSegmentXOR">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:comment >Specifies a segment of code that has been XORed with a pattern in order to avoid tripping anti-virus detection.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Code Segment XOR</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeSegment">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies an  arbitrary code segment in unencoded (plaintext or binary) format. Code would typically be included here within a CDATA section.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:label >Code Segment</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeSegmentXOR.xorPattern">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
    <rdfs:comment >Specifies a segment of code that has been XORed with a pattern in order to avoid tripping anti-virus detection.</rdfs:comment>
    <rdfs:label >XOR Pattern</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#startAddress">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
    <rdfs:comment >Specifies the start address of the code, if it was discovered inside a binary.</rdfs:comment>
    <rdfs:label >Start Address</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="processorFamily">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Processor Family</rdfs:label>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    
    <rdfs:comment >Specifies the class of processor that the code snippet is targeting.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeType">
    
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:comment >Specifies the type of code being characterized.</rdfs:comment>
    <rdfs:label >Type</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#codeDescription">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/Code&gt;</rdfs:isDefinedBy>
    
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
    <rdfs:comment >Specifies a brief description of the code that is encapsulated in this field.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Description</rdfs:label>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#extractedFeature.strings">
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
  </rdf:Description>
  <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#digitalSignatures">
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
  </rdf:Description>
  <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#extractedFeature.codeSnippets">
    <rdfs:range rdf:resource="#Code"/>
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
  </rdf:Description>
  <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#extractedFeature.imports">
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
  </rdf:Description>
  <rdf:Description rdf:about="http://cybox.mitre.org/cyboxCommon#extractedFeature.functions">
    <!-- <rdfs:domain rdf:resource="#Code"/> -->
  </rdf:Description>
</rdf:RDF>

