    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="executionProcessID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="categoryNum"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="eventType"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="generationDateTime"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="message"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="category"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="correlationActivityID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#definedEffect"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:isDefinedBy rdf:resource=""/>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#description"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="writeDateTime"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="blob"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="correlationRelatedActivityID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#state"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="user"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="executionThreadID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="source"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="index"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="eventID"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment >Characterize entries in the Windows event log.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="logName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="reserved"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#hasChanged"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="machine"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://cybox.mitre.org/cybox_core#Object"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:ID="unformattedMessages">
    <rdfs:comment >Specifies one or more unformatted messages in the event log entry.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Unformatted Messages</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#executionThreadID">
    <rdfs:comment >Specifies the Thread ID (TID) of the thread which created the event.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Execution Thread ID</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#correlationActivityID">
    <rdfs:comment >Specifies a globally unique identifier that identifies the current activity.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Correlation Activity ID</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#reserved">
    <rdfs:comment >Specifies a DWORD value that is always set to ELF_LOG_SIGNATURE (the value 0x654c664c), which is ASCII for eLfL.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Reserved</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#generationDateTime">
    <rdfs:label >Generation Time</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment >Specifies the date/time the event was generated.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#index">
    <rdfs:comment >Specifies the index of the event entry in the log.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Index</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#message">
    <rdfs:label >Message</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:comment >Specifies the rendered message string for the event.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#user">
    <rdfs:comment >Specifies the name of the user (the security ID) responsible for the event.</rdfs:comment>
    <rdfs:label >User</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#categoryNum">
    <rdfs:comment >Specifies the event entry's category number, as defined by the source.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Category Number</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#machine">
    <rdfs:comment >Specifies the name of the computer on which the event log entry was generated.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Machine</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#writeDateTime">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:label >Write Time</rdfs:label>
    <rdfs:comment >Specifies the date/time that the entry was written into the event log.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#correlationRelatedActivityID">
    <rdfs:comment >Specifies a globally unique identifier that identifies the activity to which control was transferred to.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Correlation Related Activity ID</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#eventType">
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
    <rdfs:label >Event Type</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    
    <rdfs:comment >Specifies the event type associated with the entry in the event log, e.g., warning, information, error.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#executionProcessID">
    <rdfs:comment >Specifies the Process ID (PID) of the process which created the event.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Execution Process ID</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#source">
    <rdfs:comment >Specifies the source that logged the event, typically the name of an application or sub-component.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Source</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#logName">
    <rdfs:comment >Specifies the name of the log</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Log Name</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#eventID">
    <rdfs:label >Event ID</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    
    <rdfs:comment >Specifies the ID of the event for which the event log entry was created.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#category">
    <rdfs:comment >Specifies the text associated with Category Number</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Category</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#blob">
    <rdfs:comment >Specifies the event data as a binary blob.</rdfs:comment>
    <!-- <rdfs:domain rdf:resource="#WindowsEventLog"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#base64Binary"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Blob</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>

