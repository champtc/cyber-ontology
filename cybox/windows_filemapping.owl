  <owl:ObjectProperty rdf:about="#handle">
    <rdfs:label >Handle</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:comment >Specifies the Windows handle to the file mapping.</rdfs:comment>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#fileHandle">
    <rdfs:label >File Handle</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle"/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:comment >Specifies the Windows handle to the file from which the file mapping was created.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#actualSize">
    <rdfs:comment >Specifies the actual size of the file mapping, in bytes.</rdfs:comment>
    
    <rdfs:label >Actual Size</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#filemappingName">
    <rdfs:label >Filemapping Name</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the name of the file mapping.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#securityAttributes">
    <rdfs:comment >Specifies the Windows security attributes for the file mapping.</rdfs:comment>
    
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Security Attributes</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#maximumSize">
    <rdfs:comment >Specifies the maximum allowed size for the file mapping, in bytes. This value is typically initialized upon creation of the file mapping.</rdfs:comment>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:label >Maximum Size</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#pageProtectionValue">
    <rdfs:label >Page Protection Value</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the page protection value (i.e. PAGE_) specified for the file mapping.</rdfs:comment>
    
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="pageProtectionAttribute">
    
    
    <rdfs:label >Page Protection Attribute</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsFilemapping"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies a page protection attribute (i.e. SEC_) to be used in combination with the page protection value captured in the Page Protection Value field.</rdfs:comment>
  </owl:DatatypeProperty>
</rdf:RDF>

