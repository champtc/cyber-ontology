    <!-- http://stix.mitre.org/TTP#TTP -->

    <owl:Class rdf:about="http://stix.mitre.org/TTP#TTP">
        <rdfs:subClassOf rdf:resource="http://stix.mitre.org/stixCommon#TTPBase"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stix.mitre.org/TTP#resources.infrastructure"/>
