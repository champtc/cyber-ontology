  </owl:Ontology>
  <owl:Class rdf:ID="TermsOfUseMarking">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="termsOfUse"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label >Terms of Use Marking</rdfs:label>
<!-- CS 120116 Defined in superclass MarkingStructure -->
