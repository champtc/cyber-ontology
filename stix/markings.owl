  <owl:Ontology rdf:about="http://data-marking.mitre.org/Data_Marking">
    <rdfs:comment >This ontology provides a vocabulary and representation which provides a standardized method of describing data markings used to convey sensativity and the ability to share.</rdfs:comment>
    <rdfs:label >Data Markings Ontology</rdfs:label>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1.2.0-000</owl:versionInfo>
    
    <owl:imports rdf:resource="http://stix.mitre.org/stixCommon"/>
