<?xml version="1.0"?>
<rdf:RDF xmlns="http://nvd.nist.gov/cvss#"
     xml:base="http://nvd.nist.gov/cvss"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:cvss="http://nvd.nist.gov/cvss#">
    <owl:Ontology rdf:about="http://nvd.nist.gov/cvss">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2.0.0-001</owl:versionInfo>
        <rdfs:comment>This ontology provides a vocabulary and representation for the Common Vulnerability Scoring System (CVSS) v2 which provides an open framework for communicating the characteristics and impacts of IT vulnerabilities.  CVSS consists of three groups: Base, Temporal, and Environmental. 

Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
        <rdfs:label>Common Vulnerability Scoring System (CVSS) Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://nvd.nist.gov/cvss#baseMetrics -->

    <owl:ObjectProperty rdf:about="http://nvd.nist.gov/cvss#baseMetrics">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSS"/>
        <rdfs:range rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:comment>Specifies a reference to the details for the base metrics</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Based Metrics Details</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nvd.nist.gov/cvss#environmentalMetrics -->

    <owl:ObjectProperty rdf:about="http://nvd.nist.gov/cvss#environmentalMetrics">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSS"/>
        <rdfs:range rdf:resource="http://nvd.nist.gov/cvss#EnvironmentalMetrics"/>
        <rdfs:comment>Specifies a reference to the details for environmental metrics</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Environmental Metrics Details</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nvd.nist.gov/cvss#termporalMetrics -->

    <owl:ObjectProperty rdf:about="http://nvd.nist.gov/cvss#termporalMetrics">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSS"/>
        <rdfs:range rdf:resource="http://nvd.nist.gov/cvss#TemporalMetrics"/>
        <rdfs:comment>Specifies a reference to the details for the Temporal Metrics</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Temporal Metrics Details</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://nvd.nist.gov/cvss#accessComplexity -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#accessComplexity">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HIGH</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOW</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MEDIUM</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the complexity of the attack required to exploit the vulnerability once an attacker has gained access to the target system</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>CVSS Access Complexity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#accessVector -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#accessVector">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ADJACENT NETWORK</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOCAL</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NETWORK</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies how the vulnerability is exploited</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>CVSS Access Vector</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#authenticationAttempts -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#authenticationAttempts">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MULTIPLE_INSTANCE</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NONE</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SINGLE_INSTANCE</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the number of times an attacker must authenticate to a target in order to exploit a vulnerability</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>CVSS Authentication Attempts</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#availabilityImpact -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#availabilityImpact">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">COMPLETE</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NONE</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PARTIAL</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the impact to availability of a successfully exploited vulnerability</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>CVSS Availability Impact</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#availabilityRequirement -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#availabilityRequirement">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#EnvironmentalMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HIGH</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOW</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MEDIUM</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOT_DEFINED</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the importance of the affected IT asset to a user&apos;s organization in terms of availability.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Availability Requirement (AR)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#baseScore -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#baseScore">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSSImpact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Specifies a score, between 0 and 10, that represents the intrinsic and fundamental characteristics of a vulnerability that are consistent over time and user environments.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Base Metric Score</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#baseVector -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#baseVector">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSSImpact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies a compressed textual representation that reflects the values used to drive the CVSS score for the Base Metrics</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Base Metric Vector</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#collateralDamagePotential -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#collateralDamagePotential">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#EnvironmentalMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HIGH</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOW</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOW_MEDIUM</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MEDIUM_HIGH</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NONE</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOT_DEFINED</rdf:first>
                                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the potential for losss of life or physical assets through damange or theft of peroperty of requirement.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Collateral Damange Potential (CDP)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#confidentialityImpact -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#confidentialityImpact">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">COMPLETE</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NONE</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PARTIAL</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the impact on confidentiality of a successfully exploited vulnerability</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>CVSS Confidentiality Impact</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#confidentialityRequirement -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#confidentialityRequirement">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#EnvironmentalMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HIGH</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LOW</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MEDIUM</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOT_DEFINED</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Specifies the importance of the affected IT asset to a user&apos;s organization in terms of confidentiality.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Confidentiality Requirement (CR)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#displayLabel -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#displayLabel">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#EnvironmentalMetrics"/>
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#TemporalMetrics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the text to be used as the label  in a human display</rdfs:comment>
        <rdfs:label>Display Label</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#environmentalScore -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#environmentalScore">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSSImpact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Specifies a score, between 0 and 10, that represents the characteristics of a vulnerability that change over time but not among user environments.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Environment Score</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#environmentalVector -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#environmentalVector">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#CVSSImpact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies a compressed textual representation that reflects the values used to drive the CVSS score for the Environmental Metrics</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Environmental Metric Vector</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#exploitSubscore -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#exploitSubscore">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#BaseMetrics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Specifies the base exploit sub-score asisgned to a vulnerability by a source</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://nvd.nist.gov/cvss"/>
        <rdfs:label>Exploit Subscore</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nvd.nist.gov/cvss#exploitability -->

    <owl:DatatypeProperty rdf:about="http://nvd.nist.gov/cvss#exploitability">
        <rdfs:domain rdf:resource="http://nvd.nist.gov/cvss#TemporalMetrics"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FUNCTIONAL</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HIGH</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NOT_DEFINED</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
