    <!-- http://maec.mitre.org/maec_bundle#AVClassification -->

    <owl:Class rdf:about="http://maec.mitre.org/maec_bundle#AVClassification">
        <rdfs:subClassOf rdf:resource="http://maec.mitre.org/maec_common#ToolInformation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#classificationName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#definitionVersion"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#engineVersion"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Characterizes information on AV scanner classifications for the malware instance object captured in the Bundle or Package.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://maec.mitre.org/maec_bundle"/>
        <rdfs:label>AVClassification</rdfs:label>
    </owl:Class>
    


    <!-- http://maec.mitre.org/maec_bundle#ActionCollection -->

    <owl:Class rdf:about="http://maec.mitre.org/maec_bundle#ActionCollection">
        <rdfs:subClassOf rdf:resource="http://maec.mitre.org/maec_common#BaseCollection"/>
        <rdfs:comment>Characterizes a collection of actions. This can be useful for organizing actions that may be related and where the exact relationship is unknown, as well as actions whose associated behavior has not yet been established.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://maec.mitre.org/maec_bundle"/>
        <rdfs:label>Action Collection</rdfs:label>
    </owl:Class>
    


    <!-- http://maec.mitre.org/maec_bundle#AnalysisBundle -->

    <owl:Class rdf:about="http://maec.mitre.org/maec_bundle#AnalysisBundle">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#contentType"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#timestamp"/>
