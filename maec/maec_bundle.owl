            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Specifies  any relationships between a Behavior and other Behaviors.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://maec.mitre.org/maec_bundle"/>
        <rdfs:label>Behavior relationship</rdfs:label>
    </owl:Class>
    


    <!-- http://maec.mitre.org/maec_bundle#BehavioralAction -->

    <owl:Class rdf:about="http://maec.mitre.org/maec_bundle#BehavioralAction">
        <rdfs:subClassOf rdf:resource="http://maec.mitre.org/maec_common#MalwareAction"/>
        <rdfs:subClassOf>
