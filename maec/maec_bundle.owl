            <owl:Restriction>
                <owl:onProperty rdf:resource="http://maec.mitre.org/maec_bundle#purpose"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Characterizes a collection of behaviors.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://maec.mitre.org/maec_bundle"/>
        <rdfs:label>Behavior Collection</rdfs:label>
    </owl:Class>
    


